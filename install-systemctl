sudo wget https://github.com/5jjCopter/comtrolar--navio-video-telemetria/archive/master.zip
sudo unzip master.zip
sudo rm master.zip
cd comtrolar--navio-video-telemetria-master
sudo chmod 775 video.service
sudo chmod 775 graba-video-1080-30f.service
sudo chmod 775 graba-video-720-30f.service
sudo chmod 775 video-1080.service
sudo chmod 775 video-720.service
sudo chmod 775 temperatura.service
sudo cp temperatura.service /lib/systemd/system
sudo cp graba-video-1080-30f.service /lib/systemd/system
sudo cp graba-video-720-30f.service /lib/systemd/system
sudo cp video-1080.service /lib/systemd/system
sudo cp video-720.service /lib/systemd/system
cd /home/pi

