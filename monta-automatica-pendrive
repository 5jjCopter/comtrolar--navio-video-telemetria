cd

cd monta-automatica-pendrive-master

sudo chmod 775 install-usbmount

sudo chmod 775 install-ntfs-3g

cd /home/pi/monta-automatica-pendrive-master

sudo cp graba-video /home/pi

sudo cp graba-video.service /lib/systemd/system

cd /home/pi

sudo rm -rf monta-automatica-pendrive-master

sudo chmod 775 graba-video

sudo rm -rf graba-video

